create table users (
name                varchar(30),
name_alt            varchar(30),
email               varchar(30),
address_old         varchar(200),
address_new         varchar(200),
branch              varchar(30),
location_current    varchar(30),
contact             varchar(12),
new_face            varchar(10),
familyPic1          varchar(10),
familyPic2          varchar(10),
intro               varchar(500),
password            varchar(30),
status              int(11)
);